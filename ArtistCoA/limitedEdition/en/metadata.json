 {
      "@context": "https://schema.org",
      "@type": "VisualArtwork",
      "name": "{{.AssetName}}",
      "image": "{{.ThumbnailURL}}",
      "description": "{{.CertificateLabel}}",
      "creator": [
        {
          "@type": "Person",
          "name": "{{.IssuerName}}"
        }
      ],
      "width": [
        {
          "@type": "Distance",
          "name": "{{.AssetProperties.Width}}"
        }
      ],
      "height": [
        {
          "@type": "Distance",
          "name": "{{.AssetProperties.Height}}"
        }
      ],
      "artEdition": "{{.EditionTotal}}",
      "position": "{{.EditionNumber}}",
      "artMedium": "{{.AssetProperties.ArtMedium}}",
      "artworkSurface": "{{.AssetProperties.ArtworkSurface}}"
}