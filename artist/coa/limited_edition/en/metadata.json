{
  "@context": "https://schema.org",
  "@type": "VisualArtwork",
  "name": "{{.AssetName}}",
  "image": "{{.ThumbnailURL}}",
  "description": "{{.CertificateLabel}}",
  "creator": [
    {
      "@type": "Person",
      "name": "{{.IssuerName}}"
    }
  ],
  "width": [
    {
      "@type": "Distance",
      "name": "{{.AssetProperties.Width}}"
    }
  ],
  "height": [
    {
      "@type": "Distance",
      "name": "{{.AssetProperties.Height}}"
    }
  ],
  "art_edition": "{{.EditionTotal}}",
  "position": "{{.EditionNumber}}",
  "art_medium": "{{.AssetProperties.ArtMedium}}",
  "artwork_surface": "{{.AssetProperties.ArtworkSurface}}"
}