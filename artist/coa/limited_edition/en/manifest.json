{
  "name": "limitedEdition",
  "description": "Basic Template for Limited Edition Artworks (photographic prints, ...)",
  "author":"UNCOPIED",
  "version": "1.0",
  "manifest_version": 1,
  "sourceTypes":["png","jpg"],
  "roles":["artist"],
  "countries":["*"],
  "language":"en",
  "assetProperties": {
        "height": {
                   "type": "string",
                   "optional": false
            },
        "width": {
                   "type": "string",
                   "optional": false
         },
         "artMedium": {
                   "type": "string",
                   "optional": true
         },
         "artworkSurface": {
                   "type": "string",
                   "optional": true
         },
		 "dateCreated": {
                   "type": "date",
                   "optional": false
		 }
     },
    "certificateTemplate": "legal_document.tmpl",
    "certificateLabelTemplate" : "certificate_label.txt",
    "assetLabelTemplate" : "asset_label.txt",
    "metadataTemplate" : "metadata.json",
    "editionMin":2,
    "editionMax":10000,
    "licenseTermsURL":"someURL",
    "privacyTermsURL":"someOtherURL"    
}
