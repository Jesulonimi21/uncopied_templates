{
  "id": "80b33228-463e-4bd8-a03c-77864e046ffb",
  "name": "uniqueOriginal",
  "description": "Basic Template for Unique Original Art (oil painting, statue, ...)",
  "author": "UNCOPIED",
  "version": "1.0",
  "manifest_version": 1,
  "source_types": [
    "png",
    "jpg"
  ],
  "roles": [
    "artist"
  ],
  "countries": [
    "*"
  ],
  "language": "en",
  "asset_properties": {
    "height": {
      "type": "string",
      "optional": false
    },
    "width": {
      "type": "string",
      "optional": false
    },
    "art_medium": {
      "type": "string",
      "optional": true
    },
    "artwork_surface": {
      "type": "string",
      "optional": true
    },
    "date_created": {
      "type": "date",
      "optional": false
    }
  },
  "certificate_template": "legal_document.tmpl",
  "certificate_label_template": "certificate_label.txt",
  "assetLabel_template": "asset_label.txt",
  "metadata_template": "metadata.json",
  "edition_min": 1,
  "edition_max": 1,
  "license_terms_url": "someURL",
  "privacy_terms_url": "someOtherURL"
}
